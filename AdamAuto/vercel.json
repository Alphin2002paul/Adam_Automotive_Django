{
  "builds": [
    {
      "src": "AdamAuto/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "AdamAuto/wsgi.py"
    }
  ],
  "build": {
    "env": {
      "PIP_EXTRA_INDEX_URL": "https://wheels.vercel.app"
    }
  }
}